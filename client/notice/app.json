{
  "expo": {
    "name": "notice",
    "slug": "notice",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icons/icon.png",
    "scheme": "notice",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/icons/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F2F2F2"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icons/icon.png",
        "backgroundColor": "#F2F2F2"
      },
      "package": "com.stabel.notice"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icons/favicon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-localization"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
